{
  "schemaVersion": 1,
  "id": "awesometutorial",
  "version": "${version}",

  "name": "Awesome Tutorial Mod",
  "description": "This is gonna show how to make b1.7.3 mods",
  "authors": [
    "Slainlight"
  ],
  "contact": {
    "homepage": "https://glass-launcher.net/",
    "sources": "https://github.com/calmilamsy/stationapi-example-mod"
  },

  "license": "CC0-1.0",
  "icon": "assets/awesometutorial/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "com.matthewperiut.awesometutorial.AwesomeTutorialMod"
    ],
    "stationapi:event_bus": [
      "com.matthewperiut.awesometutorial.block.TutorialBlocks",
      "com.matthewperiut.awesometutorial.recipe.AwesomeRecipes"
    ],
    "stationapi:event_bus_client": [
      "com.matthewperiut.awesometutorial.client.TextureListener"
    ],
    "stationapi:event_bus_server": [
    ]
  },
  "mixins": [
    "awesometutorial.mixins.json"
  ],

  "depends": {
    "minecraft": "1.0.0-beta.7.3"
  }
}
